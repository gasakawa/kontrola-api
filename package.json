    "jest": "^27.2.0",
    "jest-mock-extended": "^2.0.4",
    "lint-staged": "^11.2.0",
    "prettier": "^2.4.0",
    "prisma": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
