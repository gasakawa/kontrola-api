    "tsconfig-paths": "^3.11.0",
    "typescript": "4.4.3"
  },
  "scripts": {
    "build": "tsc",
    "dev:server": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/main/server.ts",
    "start": "ts-node -r tsconfig-paths/register src/main/server.ts",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "test": "jest --passWithNoTests --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests",
    "test:unit": "yarn test --watch -c jest-unit-config.js",
    "test:integration": "yarn test --watch -c jest-integration-config.js",
    "test:ci": "yarn test --coverage",
    "lint": "eslint src/** --fix",
    "prepare": "husky install",
    "load": "ts-node-dev src/infra/db/prisma/load.ts"
  },
  "prisma": {
    "schema": "src/infra/db/prisma/schema.prisma",
    "seed": "ts-node src/infra/db/prisma/seed.ts"
  }
}
